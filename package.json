{
  "name": "electron-boiler",
  "description": "Opinionated electron.atom.io boilerplate / template",
  "version": "0.0.1",
  "author": {
    "name": "Yves Schelpe",
    "email": "ys@delegate-it.be",
    "url": "http://www.delegate-it.be"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aredfox/electron-boiler"
  },
  "main": "boot.js",
  "scripts": {
    "start": "npm run build-dev && electron ./app/main",
    "prod": "npm run build-prod && electron ./app/main",
    "dev": "npm run build-dev && gulp watch",
    "build": "npm run build-dev",
    "build-dev": "gulp build --dev",
    "build-prod": "gulp build --prod",
    "clean": "gulp clean",
    "prepackage": "npm run build-prod",
    "package-win": "npm run prepackage && build -w --publish=\"never\" --dir",
    "package-mac": "npm run prepackage && build -m --publish=\"never\" --dir",
    "package-linux": "npm run prepackage && build -l --publish=\"never\" --dir"
  },
  "dependencies": {
    "dot-object": "^1.5.4",
    "moment": "^2.16.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "colors": "^1.1.2",
    "del": "^2.2.2",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "electron-reload": "^1.0.3",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-ignore": "^2.0.2",
    "gulp-json-editor": "^2.2.1",
    "gulp-less": "^3.3.0",
    "gulp-postcss": "^6.2.0",
    "gulp-relative-sourcemaps-source": "^0.1.4",
    "gulp-rename": "^1.2.2",
    "gulp-run-electron": "^3.0.1",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.11",
    "md5": "^2.2.1",
    "moment": "^2.16.0",
    "run-sequence": "^1.2.2",
    "yargs": "^6.4.0"
  },
  "build": {
    "appId": "electron-boiler",
    "category": "desktop"
  }
}
